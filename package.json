{
  "name": "@carnesen/carnesen-dot-com",
  "private": true,
  "version": "0.0.0-0",
  "description": "Automates deployment of carnesen.com to Google Cloud Platform",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "rimraf lib && tsc --project tsconfig.build.json",
    "deploy": "npm run test && node lib/deploy.js",
    "lint": "tslint --project tsconfig.build.json",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint && npm run build",
    "preversion": "npm test",
    "postversion": "npm publish"
  },
  "dependencies": {
    "@carnesen/buzz": "github:carnesen/buzz",
    "@carnesen/google-cloud": "0.0.0",
    "@carnesen/mathjs-app": "0.0.5",
    "@carnesen/meme-me": "github:carnesen/meme-me",
    "@carnesen/redirector": "0.0.0",
    "@carnesen/reveal-me": "github:carnesen/reveal-me",
    "@carnesen/run-and-exit": "0.2.0",
    "@carnesen/www": "0.2.0"
  },
  "devDependencies": {
    "@carnesen/tsconfig": "0.1.0",
    "@carnesen/tslint-config": "0.1.2",
    "@types/node": "10.12.21",
    "rimraf": "2.6.3",
    "ts-node": "8.0.2",
    "tslint": "5.12.1",
    "typescript": "3.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carnesen/carnesen-dot-com.git"
  },
  "author": "Chris Arnesen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carnesen/carnesen-dot-com/issues"
  },
  "homepage": "https://github.com/carnesen/carnesen-dot-com"
}
