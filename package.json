		"@carnesen/run-and-exit": "0.3.1",
		"@carnesen/www": "2020.6.0"
	},
	"devDependencies": {
		"@carnesen/tsconfig": "0.4.0",
		"@types/node": "14.0.23",
		"eslint": "7.4.0",
